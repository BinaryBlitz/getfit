<h1>Trainer Programs</h1>

<%= link_to 'New Trainer Program', new_trainer_program_path %>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programs.each do |program| %>
      <tr>
        <td><%= link_to 'Show', [:trainer, program] %></td>
        <td><%= link_to 'Edit', edit_trainer_program_path(program) %></td>
        <td><%= link_to 'Destroy', [:trainer, program], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
